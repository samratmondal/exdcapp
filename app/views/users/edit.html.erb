<% provide(:title, 'Edit User') %>

<div class="container">
  <div class="page-header">
    <h1>Edit User</h1>
  </div>

  <div class="row">
    <div class="span6 offset3">
    	<%= form_for(@user) do |f| %>
        <%= render(:partial => 'user_fields', :object => @user,
            :locals => { :f => f, :disable_element => false }) %>
        <br />
        <%= f.submit "Update", class: "btn btn-primary" %>
        <%= link_to "Cancel", @user, class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
